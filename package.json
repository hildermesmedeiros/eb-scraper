{
  "name": "eb-downloader",
  "version": "1.0.0",
  "description": "ArcGIS Experience Builder Downloader with Hash Verification",
  "type": "module",
  "main": "dist/main.js",
  "bin": {
    "eb-downloader": "dist/main.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "npm run build && node dist/main.js",
    "dev": "node --loader ts-node/esm src/main.ts",
    "server": "node --loader ts-node/esm src/main.ts",
    "install-global": "npm run build && npm install -g ."
  },
  "keywords": [
    "arcgis",
    "experience-builder",
    "downloader",
    "cli",
    "hash-verification",
    "puppeteer"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "commander": "^14.0.1",
    "puppeteer": "^24.22.3"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/puppeteer": "^7.0.4",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=16.13.1",
    "npm": ">=8.5.3"
  },
  "ts-node": {
    "esm": true
  }
}
